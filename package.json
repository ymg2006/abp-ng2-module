{
  "name": "@ymg2006/abp-ng2-module",
  "version": "12.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "publish": "ng build --configuration production && cd dist/abp-ng2-module && npm publish --access public && cd ../..",
